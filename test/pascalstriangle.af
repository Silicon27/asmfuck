!{5} => rows
!{1} => a0
!{0} => a1
!{0} => a2
!{0} => a3
!{0} => a4
!{0} => a5

(:rows ${

    <<@ a0
    <<@ a1
    <<@ a2
    <<@ a3
    <<@ a4
    <<@ a5

    !{a0} => n0
    !{a0 + a1} => n1
    !{a1 + a2} => n2
    !{a2 + a3} => n3
    !{a3 + a4} => n4
    !{a4 + a5} => n5

    !{n0} => a0
    !{n1} => a1
    !{n2} => a2
    !{n3} => a3
    !{n4} => a4
    !{n5} => a5
})
